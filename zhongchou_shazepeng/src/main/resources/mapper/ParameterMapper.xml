<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qf.j1808.mapper.ParameterMapper">
    <select id="selectParameter" resultType="com.qf.j1808.pojo.Parameter">
        select * from parameter
    </select>

    <select id="selectOneParameter" resultType="com.qf.j1808.pojo.Parameter">
        select * from parameter where parameterId = 1
    </select>

    <update id="updateParameter" parameterType="com.qf.j1808.pojo.Parameter">
        update parameter set parameterValue=#{parameterValue} where parameterId=#{parameterId}
    </update>
</mapper>